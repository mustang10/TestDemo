{
	"info": {
		"_postman_id": "cd15644d-1c33-48da-8dca-59688ddbb63b",
		"name": "Posts (Positive)",
		"description": "Before run this collection, you should upload the 'TestDemoData.csv' file. Attached in 'Data' folder.",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Validates that a new pet can be added to the store",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Check status', function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"//Read expected results",
							"let id = pm.variables.get('id');",
							"let categoryId = pm.variables.get('categoryId');",
							"let categoryName = pm.variables.get('categoryName');",
							"let name = pm.variables.get('name');",
							"let photoUrl = pm.variables.get('photoUrl');",
							"let tagId = pm.variables.get('tagId');",
							"let tagName = pm.variables.get('tagName');",
							"let status = pm.variables.get('status');",
							"",
							"//Read result",
							"const pet = pm.response.json();",
							"",
							"pm.test('Check that the sent id was not saved', function () {",
							"    pm.expect(pet.id).not.to.be.eq(id)",
							"});",
							"",
							"pm.test('Check that the sent category id was not saved', function () {",
							"    pm.expect(pet.category.id).not.to.be.eq(categoryId)",
							"});",
							"",
							"pm.test('Ckeck category name', function () {",
							"    pm.expect(pet.category.name).to.be.eq(categoryName)",
							"});",
							"",
							"pm.test('Ckeck name', function () {",
							"    pm.expect(pet.name).to.be.eq(name)",
							"});",
							"",
							"pm.test('Ckeck photoUrl', function () {",
							"    pm.expect(pet.photoUrls).to.includes(photoUrl)",
							"});",
							"",
							"pm.test('Check that the sent tag id was not saved', function () {",
							"    pm.expect(pet.tags[0].id).not.to.be.eq(tagId)",
							"});",
							"",
							"pm.test('Check tag name', function () {",
							"    pm.expect(pet.tags[0].name).to.deep.includes(tagName)",
							"});",
							"pm.test('Ckeck status', function () {",
							"    pm.expect(pet.status).to.be.eq(status)",
							"});"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"console.log(\"Adding \"+ pm.collectionVariables.get('name'))"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"id\": {{id}},\n  \"category\": {\n    \"id\": {{categoryId}},\n    \"name\": \"{{categoryName}}\"\n  },\n  \"name\": \"{{name}}\",\n  \"photoUrls\": [\n    \"{{photoUrl}}\"\n  ],\n  \"tags\": [\n    {\n      \"id\": {{tagId}},\n      \"name\": \"{{tagName}}\"\n    }\n  ],\n  \"status\": \"{{status}}\"\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{serverUrl}}{{endpint}}",
					"host": [
						"{{serverUrl}}{{endpint}}"
					]
				},
				"description": "Given all required data to add a new pet to the store\n\nThen the pet should be added with the given data\n\nBut the ids should be generated by the system"
			},
			"response": []
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	],
	"variable": [
		{
			"key": "endpint",
			"value": "/pet"
		}
	]
}